<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
 
 
<!--  ################################### Price Calculator Page Test Cases ##################################### -->


<suite name="Price Calculator Page Test Suite">
	<test name="Demand Page Test">
		<parameter name="browser" value="chrome"/>  
		 <classes>
			<class name="cs.qa.te.tests.PriceCalculatorPageTest">
	 
	<methods>  
			<!-- Navigates to the Price calculator Add New Price calculator page -->
       		<include name="supplierNavigateToPriceCalculatorCalculationPreviewPageTest"/>
       		
       		<!-- Clicks on Next step and navigates to the Next page and validate all the informations-->
       		<include name="supplierNavigateToPriceCalculatorNextStepPage"/>
       		
       		<!-- Validate the data in Price calculator view details page -->
       		<include name="supplierNavigateToPriceCalculatorViewDetailsPageTest"/>
       		
       		<!-- Supplier send it to the Importer and Importer Accept it -->
       		<include name="supplierSendTOfferToImporterAcceptOffer"/>		       		
     </methods>
   		
			</class>
		</classes>
	</test>
</suite>