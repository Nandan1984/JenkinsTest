<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
 
 
<!--  ################################### Supply Page Test Cases ##################################### -->

<suite name="SupplyPage and Supplier navigates Negotiations">

<test name="Importer bid on newly created Supply offer">
	<parameter name="browser" value="chrome"/>
	 <classes>
			<class name="cs.qa.te.tests.SupplyPageTest">
	 
	<methods>  
			<!-- Supplier create Supply Offer -->
       		<include name="createSupplyOfferTest"/>
       		
       		<!-- Importer Bid Supply Offer -->
       		<include name="importerBidForSupplyOffer"/>
       		
       		<!-- Supplier accept importer biddingSupply Offer -->
       		<include name="supplierAcceptImporterBidOffer"/>
       		
       		<!-- Supplier Navigate to Negotiations Deal popup -->
       		<include name="validateSupplierDealPopUpAndImporterDealPopupContractInfoTest"/>
       		<include name="validateSupplierDealPopUpAndImporterDealPopUpLogInfoTest"/>
       		<include name="validateSupplierDealPopUpAndImporterDealPopUpProcessInfoTest"/>
       		
       		<!-- Importer navigate to the Negotiations -->
       		<include name="validateAfterImporterContractSignFromSupplierTest"/>
       		<include name="validateImporterAndSupplierLogTest"/>
       		<include name="validateImporterAndSupplierProcessTest"/>
       		
       		<!-- Forwarder navigate to the Negotiations Deal popup -->
       		<include name="ForwarderNavigateDealPopUpFreightTest"/>
       		<include name="validateForwarderAndSupplierDealPopUpContractsTest"/>
       		<include name="validateForwarderAndSupplierDealPopUpProcessTest"/>
       		<include name="validateForwarderAndSupplierLogTest"/>
       		<include name="validateSupplierSignFromForwarderTest"/>
       		
       		<!-- Supplier navigate to the Active Deals -->
       		<include name="moveToActiveDeals"/>
       		<include name="validateActiveDealsContractInvoiceBetweenSupplierAndImporterTest"/>
       		<include name="validateActiveDealsContractPurchaseOrderAndProformaBetweenSupplierAndImporterTest"/>
       		<include name="validateActiveDealsProcessBetweenSupplierAndImporterTest"/>
       		<include name="validateActiveDealsLogBetweenSupplierAndImporterTest"/>
       		<include name="validateActiveDealsContractForwarderTabBetweenSupplierAndForwarderTest"/>
       		<include name="validateActiveDealsProcessForwarderTabBetweenSupplierAndForwarderTest"/>
       		<include name="validateActiveDealsLogForwarderTabBetweenSupplierAndForwarderTest"/>
       		<include name="validateActiveDealsFreightBetweenSupplierAndForwarderTest"/>
       		
     </methods>
   		
			</class>
		</classes>
		
</test>

